{
  "name": "angular-bootstrap-md",
  "version": "4.3.7",
  "description":
    "Material Design for Bootstrap (MDB) is a powerful Material Design UI KIT for most popular HTML, CSS, and JS framework - Bootstrap.",
  "main": "./bundles/angular-bootstrap-md.umd.js",
  "module": "./angular-bootstrap-md.es5.js",
  "es2015": "./angular-bootstrap-md.js",
  "typings": "./angular-bootstrap-md.d.ts",
  "author": "",
  "license": "MIT",
  "repository": {},
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "clean": "rimraf out-tsc dist/*",
    "prebuild": "npm run clean",
    "build": "node build.js",
    "lint": "tslint --project tslint.json src/**/*.ts -t verbose && stylelint src/**/*.scss",
    "release": "standard-version",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage"
  },
  "peerDependencies": {
    "@angular/core": ">=4.0.0",
    "@angular/forms": ">=4.0.0"
  },
  "devDependencies": {
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.1",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@types/jest": "^21.1.6",
    "@types/minimatch": "^3.0.1",
    "@types/node": "^6.0.46",
    "camelcase": "^4.0.0",
    "codelyzer": "^4.0.1",
    "concurrently": "3.5.0",
    "core-js": "^2.4.1",
    "glob": "^7.1.1",
    "google-closure-compiler": "^20171023.0.1",
    "jest": "^21.2.1",
    "jest-preset-angular": "^4.0.1",
    "minimatch": "^3.0.4",
    "rimraf": "^2.6.1",
    "rollup": "^0.51.3",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.5.2",
    "standard-version": "^4.0.0",
    "stylelint": "^8.2.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "~2.4.2",
    "zone.js": "^0.8.4"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/lib/setupJest.ts",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "src/lib/tsconfig.spec.json"
      },
      "__TRANSFORM_HTML__": true
    },
    "transform": {
      "^.+\\.(ts|js|html)$": "<rootDir>/node_modules/jest-preset-angular/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|/src\\/lib\\/.*\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": ["ts", "js", "html"],
    "moduleNameMapper": {
      "app/(.*)": "<rootDir>/src/app/$1",
      "assets/(.*)": "<rootDir>/src/assets/$1",
      "environments/(.*)": "<rootDir>/src/environments/$1"
    },
    "transformIgnorePatterns": ["node_modules/(?!@ngrx)"]
  }
}
